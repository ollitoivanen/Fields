<resources>

    <!-- Base application theme. -->

    <style name="FieldsThemeAppCompat" parent="Theme.AppCompat.Light" >
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:colorBackground">@color/Background_Fields</item>
    </style>

    <style name="FieldsThemeAppCompatWelcome" parent="Theme.AppCompat.Light" >
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:colorBackground">@color/Background_Fields</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.Launcher" parent="Theme.AppCompat.Light">
    <item name="android:windowBackground">@drawable/launch_screen</item>

    </style>



</resources>
